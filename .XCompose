# -*- coding: utf-8 -*-
#
# .XCompose
#

#
# Quotation marks
#
<Multi_key> <Cyrillic_be>   : "«"   guillemotleft  # LEFT DOUBLE ANGLE QUOTATION
<Multi_key> <comma>         : "«"   guillemotleft  # LEFT DOUBLE ANGLE QUOTATION
<Multi_key> <Cyrillic_yu>   : "»"   guillemotright # RIGHT DOUBLE ANGLE QUOTATION
<Multi_key> <period>        : "»"   guillemotright # RIGHT DOUBLE ANGLE QUOTATION
<Multi_key> <Cyrillic_BE>   : "„"   U201e # DOUBLE LOW-9 QUOTATION MARK
<Multi_key> <less>          : "„"   U201e # DOUBLE LOW-9 QUOTATION MARK
<Multi_key> <Cyrillic_YU>   : "“"   U201c # LEFT DOUBLE QUOTATION MARK
<Multi_key> <greater>       : "“"   U201c # LEFT DOUBLE QUOTATION MARK

#
# Dashes
#
<Multi_key> <minus>         : "—"   emdash       # EM DASH
<Multi_key> <underscore>    : "–"   endash       # EN DASH

#
# Currencies
#
<Multi_key> <space> <Cyrillic_u>        : "€"   EuroSign     # EURO SIGN
<Multi_key> <space> <e>                 : "€"   EuroSign     # EURO SIGN
<Multi_key> <space> <Cyrillic_a>        : "£"   sterling     # POUND SIGN
<Multi_key> <space> <f>                 : "£"   sterling     # POUND SIGN

#
# Trademarks
#
<Multi_key> <space> <c>               : "©"   copyright    # COPYRIGHT SIGN
<Multi_key> <space> <r>               : "®"   registered   # REGISTERED SIGN
<Multi_key> <space> <t>               : "™"   U2122        # TRADE MARK SIGN
<Multi_key> <space> <Cyrillic_es>     : "©"   copyright    # COPYRIGHT SIGN
<Multi_key> <space> <Cyrillic_ka>     : "®"   registered   # REGISTERED SIGN
<Multi_key> <space> <Cyrillic_ie>     : "™"   U2122        # TRADE MARK SIGN

#
# Math
#
<Multi_key> <space> <equal> : "≈"   approximate # ALMOST EQUAL TO
<Multi_key> <5>             : "‰"   U2030       # PER MILLE SIGN
<Multi_key> <equal>         : "≠"   U2260       # NOT EQUAL TO
<Multi_key> <plus>          : "±"   plusminus   # PLUS-MINUS SIGN
<Multi_key> <asterisk>      : "×"   U00D7       # U+00D7 MULTIPLICATION SIGN
<Multi_key> <slash>         : "÷"   U00F7       # U+00F7 DIVISION SIGN

#
# Misc. typographics
#
<Multi_key> <Cyrillic_yeru> : "§"   section      # SECTION SIG
<Multi_key> <0>             : "°"   degree       # DEGREE SIGN
<Multi_key> <space> <space> : " "   nobreakspace # NO-BREAK SPACE
<Multi_key> <space> <period>: "…"   ellipsis     # HORIZONTAL ELLIPSIS
<Multi_key> <exclam>        : "¡"   U00A1        # U+00A1 INVERTED EXCLAMATION MARK
<Multi_key> <question>      : "¿"   U00BF        # U+00BF INVERTED QUESTION MARK
<Multi_key> <backslash>     : "́"    U0301        # U+0301 COMBINING ACUTE ACCENT

#
# Missing keys in Russian layout
#
<Multi_key> <3>             : "#"   numbersign   # NUMBER SIGN
<Multi_key> <4>             : "$"   dollar       # DOLLAR SIGN

#
# Bindings to ease usage with the Russian „typewriter“ layout.
#
<Multi_key> <2>             : "—"   emdash       # EM DASH
<Multi_key> <8>             : "–"   endash       # EN DASH
<Multi_key> <percent>       : "‰"   U2030        # PER MILLE SIGN
<Multi_key> <bar>           : "±"   plusminus    # PLUS-MINUS SIGN

#
# Хальмг келн, монгол хэл, другие сибирские
#
<Multi_key> <Cyrillic_ie>   : "ә"   U04D9   # U+04D9 CYRILLIC SMALL LETTER SCHWA
<Multi_key> <Cyrillic_IE>   : "Ә"   U04D8   # U+04D8 CYRILLIC CAPITAL LETTER SCHWA
<Multi_key> <Cyrillic_er>   : "һ"   U04BB   # U+04BB CYRILLIC SMALL LETTER SHHA
<Multi_key> <Cyrillic_ER>   : "Һ"   U04BA   # U+04BA CYRILLIC CAPITAL LETTER SHHA
<Multi_key> <Cyrillic_zhe>  : "җ"   U0497   # U+0497 CYRILLIC SMALL LETTER ZHE WITH DESCENDER
<Multi_key> <Cyrillic_ZHE>  : "Җ"   U0496   # U+0496 CYRILLIC CAPITAL LETTER ZHE WITH DESCENDER
<Multi_key> <Cyrillic_en>   : "ң"   U04A3   # U+04A3 CYRILLIC SMALL LETTER EN WITH DESCENDER
<Multi_key> <Cyrillic_EN>   : "Ң"   U04A2   # U+04A2 CYRILLIC CAPITAL LETTER EN WITH DESCENDER
<Multi_key> <Cyrillic_o>    : "ө"   U04E9   # U+04E9 CYRILLIC SMALL LETTER BARRED O
<Multi_key> <Cyrillic_O>    : "Ө"   U04E8   # U+04E8 CYRILLIC CAPITAL LETTER BARRED O
<Multi_key> <Cyrillic_u>    : "ү"   U04AF   # U+04AF CYRILLIC SMALL LETTER STRAIGHT U
<Multi_key> <Cyrillic_U>    : "Ү"   U04AE   # U+04AE CYRILLIC CAPITAL LETTER STRAIGHT U

#
# Umlauts (für Deutsch, Suomea…)
#
<Multi_key> <semicolon> <o> : "ö"   U00F6   # U+00F6 LATIN SMALL LETTER O WITH DIAERESIS
<Multi_key> <colon> <o>     : "ö"   U00F6   # U+00F6 LATIN SMALL LETTER O WITH DIAERESIS
<Multi_key> <semicolon> <O> : "Ö"   U00D6   # U+00D6 LATIN CAPITAL LETTER O WITH DIAERESIS
<Multi_key> <colon> <O>     : "Ö"   U00D6   # U+00D6 LATIN CAPITAL LETTER O WITH DIAERESIS
<Multi_key> <semicolon> <u> : "ü"   U00FC   # U+00FC LATIN SMALL LETTER U WITH DIAERESIS
<Multi_key> <colon> <u>     : "ü"   U00FC   # U+00FC LATIN SMALL LETTER U WITH DIAERESIS
<Multi_key> <semicolon> <U> : "Ü"   U00DC   # U+00DC LATIN CAPITAL LETTER U WITH DIAERESIS
<Multi_key> <colon> <U>     : "Ü"   U00DC   # U+00DC LATIN CAPITAL LETTER U WITH DIAERESIS
<Multi_key> <semicolon> <a> : "ä"   U00E4   # U+00E4 LATIN SMALL LETTER A WITH DIAERESIS
<Multi_key> <colon> <a>     : "ä"   U00E4   # U+00E4 LATIN SMALL LETTER A WITH DIAERESIS
<Multi_key> <semicolon> <A> : "Ä"   U00C4   # U+00C4 LATIN CAPITAL LETTER A WITH DIAERESIS
<Multi_key> <colon> <A>     : "Ä"   U00C4   # U+00C4 LATIN CAPITAL LETTER A WITH DIAERESIS

#
# Čeština
#
<Multi_key> <j> :   "ě" U011B   # U+011B LATIN SMALL LETTER E WITH CARON
<Multi_key> <J> :   "Ě" U011A   # U+011A LATIN CAPITAL LETTER E WITH CARON
<Multi_key> <s> :   "š" U0161   # U+0161 LATIN SMALL LETTER S WITH CARON
<Multi_key> <S> :   "Š" U0160   # U+0160 LATIN CAPITAL LETTER S WITH CARON
<Multi_key> <c> :   "č" U010D   # U+010D LATIN SMALL LETTER C WITH CARON
<Multi_key> <C> :   "Č" U010C   # U+010C LATIN CAPITAL LETTER C WITH CARON
<Multi_key> <r> :   "ř" U0159   # U+0159 LATIN SMALL LETTER R WITH CARON
<Multi_key> <R> :   "Ř" U0158   # U+0158 LATIN CAPITAL LETTER R WITH CARON
<Multi_key> <z> :   "ž" U017E   # U+017E LATIN SMALL LETTER Z WITH CARON
<Multi_key> <Z> :   "Ž" U017D   # U+017D LATIN CAPITAL LETTER Z WITH CARON
<Multi_key> <y> :   "ý" U00FD   # U+00FD LATIN SMALL LETTER Y WITH ACUTE
<Multi_key> <Y> :   "Ý" U00DD   # U+00DD LATIN CAPITAL LETTER Y WITH ACUTE
<Multi_key> <a> :   "á" U00E1   # U+00E1 LATIN SMALL LETTER A WITH ACUTE
<Multi_key> <A> :   "Á" U00C1   # U+00C1 LATIN CAPITAL LETTER A WITH ACUTE
<Multi_key> <i> :   "í" U00ED   # U+00ED LATIN SMALL LETTER I WITH ACUTE
<Multi_key> <I> :   "Í" U00CD   # U+00CD LATIN CAPITAL LETTER I WITH ACUTE
<Multi_key> <o> :   "ó" U00F3   # U+00F3 LATIN SMALL LETTER O WITH ACUTE
<Multi_key> <O> :   "Ó" U00D3   # U+00D3 LATIN CAPITAL LETTER O WITH ACUTE
<Multi_key> <u> :   "ú" U00FA   # U+00FA LATIN SMALL LETTER U WITH ACUTE
<Multi_key> <U> :   "Ú" U00DA   # U+00DA LATIN CAPITAL LETTER U WITH ACUTE
<Multi_key> <e> :   "é" U00E9   # U+00E9 LATIN SMALL LETTER E WITH ACUTE
<Multi_key> <E> :   "É" U00C9   # U+00C9 LATIN CAPITAL LETTER E WITH ACUTE
<Multi_key> <w> :   "ů" U016F   # U+016F LATIN SMALL LETTER U WITH RING ABOVE
<Multi_key> <W> :   "Ů" U016E   # U+016E LATIN CAPITAL LETTER U WITH RING ABOVE
<Multi_key> <n> :   "ň" U0148   # U+0148 LATIN SMALL LETTER N WITH CARON
<Multi_key> <N> :   "Ň" U0147   # U+0147 LATIN CAPITAL LETTER N WITH CARON
<Multi_key> <D> :   "Ď" U010E   # U+010E LATIN CAPITAL LETTER D WITH CARON
<Multi_key> <d> :   "ď" U010F   # U+010F LATIN SMALL LETTER D WITH CARON
<Multi_key> <T> :   "Ť" U0164   # U+0164 LATIN CAPITAL LETTER T WITH CARON
<Multi_key> <t> :   "ť" U0165   # U+0165 LATIN SMALL LETTER T WITH CARON

#
# Старые кириллические символы, Україна, Беларусь
#
<Multi_key> <Cyrillic_hardsign> : "ѣ"   U0463   # U+0463 CYRILLIC SMALL LETTER YAT
<Multi_key> <Cyrillic_HARDSIGN> : "Ѣ"   U0462   # U+0462 CYRILLIC CAPITAL LETTER YAT
<Multi_key> <Cyrillic_i>        : "і"   U0456   # U+0456 CYRILLIC SMALL LETTER BYELORUSSIAN-UKRAINIAN I
<Multi_key> <Cyrillic_I>        : "І"   U0406   # U+0406 CYRILLIC CAPITAL LETTER BYELORUSSIAN-UKRAINIAN I
<Multi_key> <Cyrillic_shorti>   : "ї"   U0457   # U+0457 CYRILLIC SMALL LETTER YI
<Multi_key> <Cyrillic_SHORTI>   : "Ї"   U0407   # U+0407 CYRILLIC CAPITAL LETTER YI
<Multi_key> <Cyrillic_e>        : "є"   U0454   # U+0454 CYRILLIC SMALL LETTER UKRAINIAN IE (Old Cyrillic yest)
<Multi_key> <Cyrillic_E>        : "Є"   U0404   # U+0404 CYRILLIC CAPITAL LETTER UKRAINIAN IE
<Multi_key> <Cyrillic_ve>       : "ў"   U045E   # U+045E CYRILLIC SMALL LETTER SHORT U
<Multi_key> <Cyrillic_VE>       : "Ў"   U040E   # U+040E CYRILLIC CAPITAL LETTER SHORT U


